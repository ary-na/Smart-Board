<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0" spacing="15.0" styleClass="root"
      stylesheets="@../../../css/main.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="app.smartboard.controller.ConfirmNameController">
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
    <children>
        <VBox alignment="CENTER_LEFT" spacing="15.0">
            <children>
                <Label fx:id="nameLabel" text="Label"/>
                <TextField fx:id="projectNameTextField">
                    <padding>
                        <Insets bottom="6.0" left="4.0" right="4.0" top="6.0"/>
                    </padding>
                </TextField>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </VBox>
        <HBox alignment="CENTER" spacing="20.0">
            <children>
                <Button onAction="#onCancelButtonClick" fx:id="cancelButton" styleClass="cancel-button"
                        mnemonicParsing="false" text="Cancel"/>
                <Button onAction="#onConfirmButtonClick" fx:id="confirmButton" mnemonicParsing="false" text="Confirm"/>
            </children>
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </HBox>
    </children>
</VBox>
